procedure vue(taille,position:byte;xinc,yinc,zincx,zincy:integer);
begin
cleardevice;
z:=20;
zzx:=0;
zzy:=0;
while z>0 do
begin
     y:=1;
     yy:=1;
     while y<=20 do
     begin
         x:=1;
         xx:=1;
         while x<=20 do
         begin
              case position of
                   1:couleur:=tab[z,y,x];
                   2:couleur:=tab[z,x,y];
                   3:couleur:=tab[x,y,z];
                   4:couleur:=tab[x,z,y];
                   5:couleur:=tab[y,x,z];
                   6:couleur:=tab[y,z,x];
              end;{case}

              if couleur<>0 THEN
              begin
                   case taille of
                        1:final:=0;
                        2:final:=1;
                        3:final:=2;
                        4:final:=3;
                        5:final:=4;
                        6:final:=5;
                        7:final:=6;
                        8:final:=7;
                   end;{case}
                   for xplus:=0 to final do
                   begin
                        for yplus:=0 to final do
                        begin
                            if getpixel((x-xx-zzx)*taille+xplus+300,(y-yy-zzy)*taille+yplus+240)=0 then
                            putpixel((x-xx-zzx)*taille+xplus+300,(y-yy-zzy)*taille+yplus+240,couleur);
                        end;
                   end;{for}
              end;{if}
              x:=x+1;
              inc(xx,xinc);
         end;{while}
         y:=y+1;
         inc(yy,yinc);
     end;{while}
     inc(zzx,zincx);
     inc(zzy,zincy);
     z:=z-1;

end;{while}

end;{vue}

