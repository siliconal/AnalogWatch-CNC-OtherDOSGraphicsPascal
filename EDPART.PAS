Procedure Retourline;
begin
     if colcourant>625 then
     begin
          colcourant:=5;
          inc(linecourant,40);
          case clecourant of
               2:audessus:=2;
               3:audessus:=3;
               4:audessus:=3;
               5:audessus:=-5;
               6:audessus:=-1;
               7:audessus:=3;
               8:audessus:=7;
          end;
          cle(colcourant,linecourant-audessus,20,20,clecourant-1);
          inc(colcourant,15);
     end;
end;

procedure DesCle(clecourant:byte);
begin
     effsouris;
     case clecourant of
          2:audessus:=2;
          3:audessus:=3;
          4:audessus:=3;
          5:audessus:=-5;
          6:audessus:=-1;
          7:audessus:=3;
